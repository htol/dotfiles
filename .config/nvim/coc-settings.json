{
    "python.jediEnabled": false,
    "languageserver": {
        "python": {
            "command": "python",
            "args": [
                "-mpyls",
                "-vv",
                "--log-file",
                "/tmp/lsp_python.log"
            ],
            "trace.server": "verbose",
            "filetypes": [
                "python"
            ],
            "settings": {
                "pyls": {
                    "enable": true,
                    "trace": {
                        "server": "verbose"
                    }
                }
            }
        },

        "golang": {
            "trace.server":"verbose",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "disableDynamicRegister":true,
            "coc.preferences.colorSupport": false,
            "initializationOptions": {
                "usePlaceholders": true
            }
      }
    }
}
